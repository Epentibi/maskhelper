<!--设置轮播-->
<swiper previous-margin="40rpx" next-margin="40rpx" circular="true" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" style="height: 500rpx;" >
  <block wx:for="{{imgUrls}}" >  
      <swiper-item class='slideshow'>
        <image src="{{item}}" class='swiperimg'/>
      </swiper-item>
  </block>
</swiper>


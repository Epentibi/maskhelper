<!--pages/home/home.wxml-->





<view class='robotsearch'>
      <image src="../../images/Search Menu/Robot.png" style='width:100rpx;height:100rpx'></image>
      <input class='robotinput' placeholder="请输入你的口罩品牌" ></input>
      <button class='robotbutton' style='width:250rpx;height:80rpx'>智能搜索</button>
</view>

<view class='breakline'></view>
<swiper circular="true" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
  <block wx:for="{{imgUrls}}">  
      <swiper-item>
        <image src="{{item}}" class='slideshow' model="aspectFit" />
      </swiper-item>
  </block>
</swiper>
<view class='mansearch'>
    <image src="../../images/Search Menu/Officer.png" style='width:95rpx;height:90rpx'></image>
    <text class='maninfo'>请在下面查找你的型号</text>
    <text class='manbutton'size="mini">人工识别</text>
</view>   


  <view class='listname'>
    <view>口罩型号栏</view>
  </view>
  <view class='hr2'></view>
  <import src="../template/template.wxml"></import>
  <view>
    <template is='searchlist' data="{{array}}"/>


  </view>


